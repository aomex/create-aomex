datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model user {
  id         Int      @id @default(autoincrement())
  name       String   @unique
  age        Int      @default(17)
  address    String?
  created_at DateTime @default(now()) @db.Timestamp(0)
  updated_at DateTime @default(now()) @updatedAt() @db.Timestamp(0)
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int      @id @default(autoincrement())
  name       String
  age        Int
  created_at DateTime @default(now())
  updated_at DateTime @default(now()) @updatedAt()
}
